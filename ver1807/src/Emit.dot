       IF e#NIL THEN IR.PrintAttributes (e^.attr^); END; WriteLn; 
       WriteNest (nest); 
       RegAlloc.PrintAllocation (1); WriteLn;
    END;

	CheckRoundMode (RndZero);
.	fistpl	{m AT0.mem.am}
	PopFStack;
	
     | 62 :
    IF (RegAlloc.allocation [1]. num>0) THEN 
